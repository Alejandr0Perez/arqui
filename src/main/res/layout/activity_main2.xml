<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:columnCount="2"
    android:rowCount="2"
    android:layout_margin="16dp"
    tools:context=".MainActivity2">

    <Button
        android:id="@+id/btnCotizaciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cotizaciones"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="36dp" />

    <Button
        android:id="@+id/btnMateriales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Materiales"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="325dp" />

    <Button
        android:id="@+id/btnEmpleados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Empleados"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="544dp" />

    <Button
        android:id="@+id/btnVerHistorial"
        android:layout_width="130dp"
        android:layout_height="46dp"
        android:text="Historial"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="150dp" />


</androidx.constraintlayout.widget.ConstraintLayout>